<odoo>
    <data>
        <record id="wizard_create_variant_view" model="ir.ui.view">
            <field name="name">wizard.create.variant.form</field>
            <field name="model">wizard.create.variant</field>
            <field name="arch" type="xml">
                <form string="Create New Variant">
                    <sheet>
                        <group>
                            <group>
                                <field name="omna_product_id" invisible="1"/>
                                <field name="omna_variant_id" invisible="1"/>
                                <!--<field name="external_id_integration_ids" invisible="1"/>-->
                                <field name="name"/>
                                <field name="default_code"/>
                                <field name="description"/>
                            </group>
                            <group>
                                <field name="lst_price"/>
                                <field name="standard_price"/>
                                <field name="product_template_id" invisible="1"/>
                                <!--<field name="external_id_integration_ids" invisible="1"/>-->
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button string="Create New Variant" name="create_variant"
                                type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window id="wizard_create_variant"
            multi="True"
            key2="client_action_multi" name="Create New Variant"
            res_model="wizard.create.variant" src_model="product.template"
            view_mode="form" target="new" view_type="form" />

    </data>
</odoo>
